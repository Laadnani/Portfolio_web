{
  "extends": [
    "development"
  ],
  "hints": {
    "manifest-file-extension": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 113",
    "not ios_saf <= 14.8",
    "not opera <= 98",
    "not safari <= 14.1"
  ]
}